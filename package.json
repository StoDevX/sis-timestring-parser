{
  "name": "sto-sis-time-parser",
  "description": "A parser for the St. Olaf SIS timestrings.",
  "version": "2.0.0",
  "repository": "hawkrives/sto-sis-time-parser",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "build": "babel lib --out-dir dist && node bin/make_index.js",
    "clean": "rm -rf dist/ index.js",
    "postpublish": "npm run clean",
    "prepublish": "npm run build",
    "push": "./bin/publish.sh",
    "test": "mocha",
    "watch": "babel --watch lib --out-dir dist"
  },
  "dependencies": {
    "lodash": "^3.0.0"
  },
  "devDependencies": {
    "babel": "^5.8.21",
    "chai": "^1.10.0",
    "mocha": "^2.1.0"
  },
  "keywords": [
    "sto-scheduler",
    "stolaf"
  ]
}
