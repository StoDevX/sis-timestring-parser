{
  "name": "sto-sis-time-parser",
  "description": "A parser for the St. Olaf SIS timestrings.",
  "version": "1.0.9",
  "repository": "hawkrives/sto-sis-time-parser",
  "author": "Hawken M. Rives",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "push": "./bin/publish.sh",
    "build": "6to5 lib --out-dir dist && node bin/make_index.js",
    "clean": "rm -rf dist/ index.js",
    "watch": "6to5 --watch lib --out-dir dist",
    "test": "mocha",
    "prepublish": "npm run build",
    "postpublish": "npm run clean"
  },
  "dependencies": {
    "lodash": "^3.0.0"
  },
  "devDependencies": {
    "6to5": "^3.0.5",
    "chai": "^1.10.0",
    "mocha": "^2.1.0"
  },
  "keywords": [
    "stolaf",
    "sto-scheduler"
  ]
}
